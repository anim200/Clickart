{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
    "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "https://clickart-admin.vercel.app" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" }
      ]
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
